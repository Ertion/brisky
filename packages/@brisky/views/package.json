{
  "name": "@brisky/views",
  "version": "0.0.1",
  "briskySystem": true,
  "private": true,
  "scripts": {
    "brisky:serve": "vue-cli-service serve -v packages/@brisky/views/views.conf.ts -t  packages/@brisky/views/public/pages/index.html -d",
    "brisky:build": "vue-cli-service build --no-clean -v packages/@brisky/views/views.conf.ts -t  packages/@brisky/views/public/pages/index.html",
    "serve": "vue-cli-service serve -v views.conf.ts -d",
    "build": "vue-cli-service build --no-clean -v views.conf.ts ",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@brisky/api": "^0.0.1",
    "@brisky/core": "^0.0.1",
    "@brisky/eventbus": "^0.0.1",
    "@brisky/util": "^0.0.1",
    "core-js": "^3.8.3",
    "vue": "^3.0.4"
  },
  "devDependencies": {
    "@brisky/vue-cli-plugin-pack": "^0.0.1",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.4.13",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "@vue/cli-plugin-babel": "~5.0.0-beta.2",
    "@vue/cli-plugin-eslint": "~5.0.0-beta.2",
    "@vue/cli-plugin-typescript": "~5.0.0-beta.2",
    "@vue/cli-service": "~5.0.0-beta.2",
    "@vue/compiler-sfc": "^3.0.4",
    "@vue/eslint-config-typescript": "^7.0.0",
    "commander": "^8.1.0",
    "eslint": "^7.20.0",
    "eslint-plugin-vue": "^7.2.0",
    "sass": "^1.32.7",
    "sass-loader": "^11.0.1",
    "typescript": "~4.1.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "@typescript-eslint/no-var-requires": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-empty-function": 0,
      "@typescript-eslint/ban-ts-comment": 0,
      "no-debugger": 0,
      "no-console": 0
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
