{
  "name": "@brisky/package-template",
  "version": "0.0.0",
  "description": "@brisky/package-template",
  "main": "dist/brisky-package-template.cjs.js",
  "unpkg": "dist/brisky-package-template.global.js",
  "jsdelivr": "dist/brisky-package-template.global.js",
  "module": "dist/brisky-package-template.esm-bundler.js",
  "umd": "dist/brisky-package-template.umd.js",
  "types": "dist/brisky-package-template.d.ts",
  "outputName": "BriskyPackageTemplate",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint \"src/**\" --fix  --ext .js，.jsx，.ts，.tsx --config .eslintrc.js ",
    "build": "npm run \"build:ts\" && npm run build:dts",
    "build:ts": "rollup -c rollup.config.js",
    "build:dts": "api-extractor run --local --verbose && tail -n +5 src/globalExtensions.ts >> dist/brisky-package-template.d.ts ",
    "show":"yalc installations show",
    "async": "npm run build && yalc push --no-scripts",
    "watch": "nodemon --ignore dist/ --ignore node_modules/ --watch src/ -C -e ts,tsx,scss -x \"npm run async\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xhl592576605/brisky.git"
  },
  "keywords": [],
  "files": [
    "dist/*.js",
    "dist/brisky-package-template.d.ts",
    "README.md"
  ],
